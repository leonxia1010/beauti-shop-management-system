{
  "name": "@beauty-shop/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "lint": "nx run-many --target=lint --all",
    "test": "nx run-many --target=test --all",
    "build": "nx run-many --target=build --all",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,html,css,scss}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,html,css,scss}\"",
    "prepare": "husky",
    "dev:api": "PORT=3010 nx serve api-gateway",
    "dev:web": "nx serve admin-web",
    "dev:mobile": "nx serve mobile-app",
    "dev:jobs": "nx serve batch-jobs",
    "db:start": "docker compose up postgres redis -d",
    "db:stop": "docker compose down",
    "db:reset": "docker compose down -v && docker compose up postgres redis -d",
    "db:logs": "docker compose logs postgres redis",
    "docker:up": "docker compose up --build -d",
    "docker:down": "docker compose down",
    "check": "npm run lint && npm run test && npm run format:check",
    "fix": "npm run format && npm run lint --fix",
    "clean": "nx reset && rm -rf dist",
    "graph": "nx graph",
    "affected:test": "nx affected --target=test",
    "affected:lint": "nx affected --target=lint",
    "affected:build": "nx affected --target=build"
  },
  "private": true,
